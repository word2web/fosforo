{
  "name": "lightningcss-wasm",
  "version": "1.29.3",
  "license": "MPL-2.0",
  "description": "A CSS parser, transformer, and minifier written in Rust",
  "main": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "node": {
        "import": "./wasm-node.mjs",
        "require": "./wasm-node.cjs"
      },
      "default": {
        "import": "./index.mjs",
        "require": "./index.cjs"
      }
    },
    "./lightningcss_node.wasm": "./lightningcss_node.wasm"
  },
  "browserslist": "last 2 versions, not dead",
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/parcel"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parcel-bundler/lightningcss.git"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "files": [
    "*.js",
    "*.cjs",
    "*.mjs",
    "*.d.ts",
    "*.flow",
    "*.wasm"
  ],
  "dependencies": {
    "napi-wasm": "^1.0.1"
  },
  "resolutions": {
    "lightningcss": "link:."
  },
  "type": "module",
  "module": "index.mjs",
  "sideEffects": false,
  "bundledDependencies": [
    "napi-wasm"
  ]
}
